{
    "models": [
        "qwen/qwen-vl-plus:free",
        "sophosympatheia/rogue-rose-103b-v0.2:free",
        "meta-llama/llama-3.2-3b-instruct:free"
    ],
    "aggregator": [
        {
            "model": "deepseek/deepseek-r1:free",
